{
  "name": "todoapi",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "db:up": "./node_modules/.bin/db-migrate up",
    "db:down": "./node_modules/.bin/db-migrate down",
    "db:start": "pg_ctl -D /usr/local/var/postgres start",
    "db:stop": "pg_ctl -D /usr/local/var/postgres stop"
  },
  "dependencies": {
    "body-parser": "~1.15.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.4.0",
    "dotenv": "^2.0.0",
    "express": "~4.14.1",
    "jade": "~1.11.0",
    "massive": "^2.5.0",
    "morgan": "~1.7.0",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "db-migrate": "^0.10.0-beta.20",
    "db-migrate-pg": "^0.1.11"
  }
}
